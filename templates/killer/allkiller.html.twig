{% extends 'base.html.twig' %}

{% block title %}{% endblock %}
 
{% block body %}

<div class="container-fluid">
    <div class="container">
        <h1> Tous les Killers </h1>
    </div>

    {% for killer in killers %}

    <div class="container">

        <a class="text-decoration-none" href="{{ path('killer_show', { slug: killer.slug }) }}" ><h1>{{ killer.name }}</h1></a>

    </div>

</div>

{% endfor %}

{% endblock %}